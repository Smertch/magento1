<?xml version="1.0"?>
<layout version="1.0.0.0">

    <block type="page/html" name="root" output="toHtml" template="page/print.phtml">

        <block type="page/html_head" name="head" as="head">
            <action method="addJs"><script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB-EiMRj3eD06X_IWL3ZJvDjyRJEwCE95Y"></script></action>
            <action method="addCss"><stylesheet>css/map.css</stylesheet></action>
        </block>


    </block>
    <storelocator_storelocator_index>
        <reference name="head">
            <action method="setTitle">
                <title>Store locator</title>
            </action>
        </reference>
        <reference name="content">
            <block name="storelocator.stor"  type="storelocator/stor" template="storelocator/stor.phtml"/>
        </reference>
    </storelocator_storelocator_index>
    <storelocator_storelocator_information>
        <reference name="head">
            <action method="setTitle">
                <title>Store information</title>
            </action>
        </reference>
        <reference name="content">
            <block name="storelocator.stor"  type="storelocator/stor" template="storelocator/storeinformation.phtml"/>
        </reference>
    </storelocator_storelocator_information>

</layout>